<%= simple_form_for [@hotel, @room] do |f| %>
  <%= f.input :category %>
  <%= f.input :capacity %>
  <%= f.input :price %>
  <%= f.fields_for :room_attachments do |p| %>
    <%= p.label :photo %>
    <%= p.file_field :photo, :multiple => true, name: "room_attachments[photo][]" %>
    <% end %>
  <%= f.button :submit %>
<% end %>
